{
  "name": "init",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "history": "^4.7.2",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-avatar-editor": "^11.0.4",
    "react-datepicker": "^2.0.0",
    "react-dom": "^16.6.3",
    "react-dropzone": "^7.0.1",
    "react-redux": "^5.1.1",
    "react-responsive-modal": "^3.5.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "react-select": "^2.1.2",
    "redux": "^4.0.1",
    "tcomb-form": "^0.9.21"
  },
  "devDependencies": {
    "babel-cli": "^6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7",
    "babel-plugin-module-resolver": "^2",
    "babel-polyfill": "^6.0.16",
    "babel-preset-es2015": "^6",
    "babel-preset-es2017": "^6",
    "babel-preset-react": "^6",
    "babel-preset-react-app": "^3.1.0",
    "babel-preset-stage-0": "^6",
    "babel-register": "^6",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.4.0",
    "html-webpack-plugin": "^2",
    "node-sass": "^4",
    "postcss-loader": "^2",
    "sass-loader": "^6",
    "style-loader": "^0",
    "webpack": "^3",
    "webpack-dev-server": "^2"
  },
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base dist/ --inline --hot",
    "build": "NODE_ENV=production webpack -d",
    "open": "electron index.js",
    "pack": "electron-builder",
    "watch": "webpack -d --watch",
    "lint": "eslint './**/*.js'"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "plugins": [
      "import"
    ],
    "settings": {
      "import/resolver": {
        "babel-module": {}
      }
    },
    "rules": {
      "max-len": [
        "error",
        100,
        2,
        {
          "ignoreComments": false,
          "ignoreStrings": false,
          "ignoreTemplateLiterals": false
        }
      ],
      "react/jsx-filename-extension": "off"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "es2017",
      "stage-0",
      "react"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "alias": {
            "actions": "../common/redux/actions",
            "config": "../common/config",
            "store": "../common/redux/store",
            "super_container": "../common/super_container",
            "InputComponent": "./src/components/forms/inputText",
            "forms": "./src/components/forms",
            "models": "../common/models",
            "common": "../common"
          }
        }
      ]
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
